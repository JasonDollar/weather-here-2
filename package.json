{
  "name": "weather-here-2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons-weather": "^1.0.5",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "styled-icons": "^10.25.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": [
        "warn"
      ],
      "no-unreachable": [
        "warn"
      ],
      "eqeqeq": [
        "error"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-use-before-define": [
        "error"
      ],
      "no-const-assign": [
        "error"
      ],
      "no-var": [
        "error"
      ],
      "prefer-const": [
        0
      ],
      "semi": [
        1,
        "never"
      ],
      "padded-blocks": [
        "off"
      ],
      "no-trailing-spaces": [
        "off"
      ],
      "spaced-comment": [
        "off"
      ],
      "eol-last": [
        "off"
      ],
      "no-multiple-empty-lines": [
        2,
        {
          "max": 3
        }
      ],
      "no-param-reassign": [
        1,
        {
          "props": false
        }
      ],
      "no-return-assign": [
        0
      ],
      "max-len": [
        1
      ],
      "no-restricted-globals": [
        0
      ],
      "arrow-parens": [
        2,
        "as-needed"
      ],
      "radix": [
        2,
        "as-needed"
      ],
      "no-unused-expressions": [
        2,
        {
          "allowTernary": true
        }
      ],
      "linebreak-style": [
        0
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-boolean-value": [
        "off"
      ],
      "prefer-arrow-callback": [
        "off"
      ],
      "prefer-template": [
        "off"
      ],
      "no-underscore-dangle": [
        "off"
      ],
      "react/jsx-indent": [
        "error", 2
      ],
      "react/jsx-one-expression-per-line": [
        "off"
      ],
      "consistent-return": [
        "off"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
